:ConceptScheme_1
  rdf:type skos:ConceptScheme ;
  terms:spatial <http://bla.bla/bla> ;
  terms:spatial <http://publications.europa.eu/resource/authority/continent/EUROPE> ;
  rdfs:comment "Concept scheme 1" ;
  rdfs:label "Concept scheme 1" ;
  owl:versionInfo "1" ;
  dcat:theme ontology:LegalValue-authoritative ;
  dcat:theme <http://publications.europa.eu/resource/authority/continent/ASIA> ;
  dcat:theme <http://publications.europa.eu/resource/authority/data-theme/ENVI> ;
.
:Concept_1
  rdf:type skos:Concept ;
  rdfs:comment "Concept 1" ;
  rdfs:label "Concept 1" ;
  owl:versionInfo "1" ;
.
:ContinentRestriction
  rdf:type rdfs:Class ;
  rdf:type sh:Shape ;
  rdfs:comment "Continent restriction" ;
  rdfs:label "Continent restriction" ;
  rdfs:subClassOf rdfs:Resource ;
  owl:versionInfo "1" ;
  sh:property [
      sh:predicate skos:inScheme ;
      sh:class skos:ConceptScheme ;
      sh:hasValue <http://publications.europa.eu/resource/authority/data-theme> ;
      sh:minCount 1 ;
      sh:name "in scheme" ;
      sh:nodeKind sh:IRI ;
    ] ;
.
:DataThemeRestriction
  rdf:type rdfs:Class ;
  rdf:type sh:Shape ;
  rdfs:comment "Data theme restriction" ;
  rdfs:label "Data theme restriction" ;
  rdfs:subClassOf rdfs:Resource ;
  owl:versionInfo "1" ;
  sh:property [
      sh:predicate skos:inScheme ;
      sh:class skos:ConceptScheme ;
      sh:hasValue atold-res:data-theme ;
      sh:minCount 1 ;
      sh:name "in scheme" ;
      sh:nodeKind sh:IRI ;
    ] ;
.
skos:Concept
  rdf:type sh:Shape ;
.
skos:ConceptScheme
  rdf:type sh:Shape ;
  sh:property [
      sh:predicate terms:spatial ;
      sh:class skos:Concept ;
      sh:name "spatial" ;
      sh:nodeKind sh:IRI ;
      sh:shape :ContinentRestriction ;
    ] ;
  sh:property [
      sh:predicate dcat:theme ;
      sh:class skos:Concept ;
      sh:name "theme" ;
      sh:nodeKind sh:IRI ;
      sh:shape [
          rdf:type sh:Shape ;
          sh:or (
              :DataThemeRestriction
              :ContinentRestriction
            ) ;
        ] ;
    ] ;
