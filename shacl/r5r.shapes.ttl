:Catalogue
  rdf:type sh:Shape ;
  dcterms:identifier "dcat-ap:Catalogue" ;
  vann:usageNote "Mandatory class. A catalogue or repository that hosts the Datasets being described."@en ;
  rdfs:comment "A curated collection of metadata about datasets"@en ;
  rdfs:isDefinedBy : ;
  rdfs:label "Catalogue"@en ;
  rdfs:seeAlso <https://www.w3.org/TR/vocab-dcat/#class-catalog> ;
  vs:term_status "stable" ;
  sh:description "A curated collection of metadata about datasets"@en ;
  sh:name "Catalogue"@en ;
  sh:property [
      sh:predicate dcterms:description ;
      sh:description "A free-text account of the catalog."@en ;
      sh:message "dct:Catalogue must have at least one description" ;
      sh:minCount 1 ;
      sh:name "description" ;
      sh:nodeKind sh:Literal ;
      sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:predicate dcterms:hasPart ;
      sh:class dcat:Catalog ;
      sh:message "the catalogue parts must be other catalogues" ;
      sh:name "has part" ;
      sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:predicate dcterms:hasPart ;
      sh:message "A catalogue may have as parts other catalogues" ;
      sh:minCount 1 ;
      sh:name "has part" ;
      sh:severity sh:Info ;
    ] ;
  sh:property [
      sh:predicate dcterms:isPartOf ;
      sh:class dcat:Catalog ;
      sh:description "The phisical or logical inclussion of a Catalog into another one" ;
      sh:maxCount 1 ;
      sh:message "dct:Catalogue must be part of maximum one other Catalogue" ;
      sh:name "is part of" ;
      sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:predicate dcterms:isPartOf ;
      sh:message "A Catalogue may be a part of another one. " ;
      sh:minCount 1 ;
      sh:name "is part of" ;
      sh:severity sh:Info ;
    ] ;
  sh:property [
      sh:predicate dcterms:issued ;
      sh:datatype xsd:date ;
      sh:description "Date of formal issuance (e.g., publication) of the catalog." ;
      sh:maxCount 1 ;
      sh:message "dct:Catalogue must have maximum one release date" ;
      sh:name "issued" ;
      sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:predicate dcterms:issued ;
      sh:message "A Catalogue should have an issued date" ;
      sh:minCount 1 ;
      sh:name "issued" ;
      sh:severity sh:Warning ;
    ] ;
  sh:property [
      sh:predicate dcterms:language ;
      sh:class dcterms:LinguisticSystem ;
      sh:message "A catalogue's language must be a LinguisticSystem" ;
      sh:name "language" ;
      sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:predicate dcterms:language ;
      sh:message "A Catalogue shall have a language property" ;
      sh:minCount 1 ;
      sh:name "language" ;
      sh:severity sh:Warning ;
    ] ;
  sh:property [
      sh:predicate dcterms:license ;
      sh:class dcterms:LicenseDocument ;
      sh:description "This links to the license document under which the distribution is made available." ;
      sh:maxCount 1 ;
      sh:message "dct:Catalogue must have maximum one license" ;
      sh:name "license" ;
      sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:predicate dcterms:license ;
      sh:class dcterms:LicenseDocument ;
      sh:message "A catalogue's license must be a Licensedocument" ;
      sh:name "license" ;
      sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:predicate dcterms:license ;
      sh:message "A catalogue should have a license property" ;
      sh:minCount 1 ;
      sh:name "license" ;
      sh:severity sh:Warning ;
    ] ;
  sh:property [
      sh:predicate dcterms:modified ;
      sh:datatype xsd:date ;
      sh:description "Most recent date on which the catalog was changed, updated or modified." ;
      sh:maxCount 1 ;
      sh:message "dct:Catalogue must have maximum one modification date" ;
      sh:name "modified" ;
      sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:predicate dcterms:modified ;
      sh:message "A Catalogue should have a modified date" ;
      sh:minCount 1 ;
      sh:name "modified" ;
      sh:severity sh:Warning ;
    ] ;
  sh:property [
      sh:predicate dcterms:publisher ;
      sh:class foaf:Agent ;
      sh:description "An entity responsible for making the catalog available." ;
      sh:maxCount 1 ;
      sh:message "dct:Catalogue must have exactly one publisher" ;
      sh:minCount 1 ;
      sh:name "publisher" ;
    ] ;
  sh:property [
      sh:predicate dcterms:publisher ;
      sh:class foaf:Agent ;
      sh:message "dct:Catalogue must have at least one publisher" ;
      sh:name "publisher" ;
      sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:predicate dcterms:rights ;
      sh:class dcterms:RightsStatement ;
      sh:description "This describes the rights under which the catalog can be used/reused and not the datasets. Even if theses rights apply to all the catalog datasets and distributions, it should be replicated on each distribution." ;
      sh:maxCount 1 ;
      sh:message "dct:Catalogue must have maximum one rights statement" ;
      sh:name "rights" ;
      sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:predicate dcterms:rights ;
      sh:message "The Catalogue may have rights propoerty" ;
      sh:minCount 1 ;
      sh:name "rights" ;
      sh:severity sh:Info ;
    ] ;
  sh:property [
      sh:predicate dcterms:spatial ;
      sh:class dcterms:Location ;
      sh:message "The spatial propoerty of a Catalogue must be a Location" ;
      sh:name "spatial" ;
      sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:predicate dcterms:spatial ;
      sh:message "A Catalogue may have a spatial property" ;
      sh:minCount 1 ;
      sh:name "spatial" ;
      sh:severity sh:Info ;
    ] ;
  sh:property [
      sh:predicate dcterms:title ;
      sh:description "A name given to the catalog."@en ;
      sh:message "dct:Catalogue must have at least one title" ;
      sh:minCount 1 ;
      sh:name "title" ;
      sh:nodeKind sh:Literal ;
      sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:predicate dcat:dataset ;
      sh:class dcat:Dataset ;
      sh:description "A dataset that is part of the catalog." ;
      sh:message "dct:Catalogue must have at least one dataset" ;
      sh:minCount 1 ;
      sh:name "dataset" ;
      sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:predicate dcat:record ;
      sh:class dcat:CatalogRecord ;
      sh:message "Catalogue records must be of type CatalogRecords" ;
      sh:name "record" ;
      sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:predicate dcat:record ;
      sh:message "The Catalogue may have record property" ;
      sh:minCount 1 ;
      sh:name "record" ;
      sh:severity sh:Info ;
    ] ;
  sh:property [
      sh:predicate dcat:themeTaxonomy ;
      sh:class skos:ConceptScheme ;
      sh:message "A catalogue's themeTaxonomy must be a ConceptScheme" ;
      sh:name "theme taxonomy" ;
      sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:predicate dcat:themeTaxonomy ;
      sh:message "A Catalogue should have a themeTaxonomy property" ;
      sh:minCount 1 ;
      sh:name "theme taxonomy" ;
      sh:severity sh:Warning ;
    ] ;
  sh:property [
      sh:predicate foaf:homepage ;
      sh:class foaf:Document ;
      sh:description "The homepage of the catalog." ;
      sh:maxCount 1 ;
      sh:message "dct:Catalogue must have maximum one homepage" ;
      sh:name "homepage" ;
      sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:predicate foaf:homepage ;
      sh:class foaf:Document ;
      sh:message "Catalogue's homepage shall be a foaf:Document" ;
      sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:predicate foaf:homepage ;
      sh:message "A Catalogue shall have a homepage" ;
      sh:minCount 1 ;
      sh:severity sh:Warning ;
    ] ;
  sh:targetClass dcat:Catalog ;
.
